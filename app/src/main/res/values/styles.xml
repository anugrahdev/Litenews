<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/almost_white</item>
        <item name="colorPrimaryDark">@color/almost_white</item>
        <item name="colorAccent">@color/main_blue</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@color/colorTransparenSoftWhite</item>

        <item name="textcolor">@color/darkest_gray</item>
        <item name="bgcolor">@color/almost_white</item>
        <item name="toolbarcolor">@color/almost_white</item>
        <item name="bottomnavcolor">@color/almost_white</item>
        <item name="bottomselected">@color/main_blue</item>
        <item name="bottomnotselected">@color/nobel</item>
        <item name="dividercolor">@color/cararra</item>
        <item name="edittextbg">@color/cararra</item>
    </style>


</resources>
